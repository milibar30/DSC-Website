          <!---Contact Us Section Starts-->
          <section class="contact_us" id="contactus">
            <!-- <img src="../public/img/illustrations/contact.png" alt="" /> -->
            <div style="margin-left:30vw;">
              <% if (flashMessages) { %>
                <% if (flashMessages.contact_success) { %>
                  <!-- <div class="flash success"><%= flashMessages.success %></div> -->
                  <div class="alert" id="success">
                    <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                    <%= flashMessages.contact_success %>
                  </div>
                <% } else if (flashMessages.contact_error) { %>
                  <div class="alert" id="error">
                    <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
                    <%= flashMessages.contact_error %>
                  </div>
                <% } %>
              <% } %>
            </div>
            <h2 class="heading__primary intersect">
              Contact Us
            </h2>
            <form action="/dsc/contact" method="post" id="upload_form" enctype="multipart/form-data">
              <div class="container">
                <div class="row">
                  <h4 style="text-align: center;">We'd love to hear from you!</h4>
                </div>
                <div class="row input-container">
                  <div class="col-xs-12">
                    <div class="styled-input wide">
                      <input type="text" class="full" name="name" required/>
                      <label>Name</label>
                    </div>
                  </div>
                  <div class="col-md-6 col-sm-12">
                    <div class="styled-input">
                      <input type="text" class="full" name="email" required/>
                      <label>Email</label>
                    </div>
                  </div>
                  <div class="col-md-6 col-sm-12">
                    <div class="styled-input" style="float: right;">
                      <input type="text" class="full" name="subject" required />
                      <label>Subject</label>
                    </div>
                  </div>
    
                  <div class="col-xs-12">
                    <div class="styled-input wide">
                      <input type="text" class="message full" name="message" required />
                      <label>Message</label>
                    </div>
                  </div>
                  <center>
                    <div class="col-xs-12">
                      <label for="file" id="lb" class="custom-file-upload"
                        >Upload a file</label
                      >
                      <div class="styled-input wide">
                        <input
                          class="inputfile"
                          type="file"
                          name="issues"
                          id="file1"
                          onchange="uploadFile()"
                          multiple
                        /><br />
                        <center>
                          <progress
                            class="progress"
                            id="progressBar"
                            value="0"
                            max="100"
                            style="width: 300px;"
                          ></progress>
                        </center>
    
                        <h3 id="status" class="status"></h3>
                        <p id="loaded_n_total" class="loaded_n_total"></p>
                      </div>
                    </div>
    
                    <div class="col-xs-12">
                      <button class="btn-lrg submit-btn">Send Message</button>
                    </div>
                  </center>
                </div>
              </div>
            </form>
          </section>



